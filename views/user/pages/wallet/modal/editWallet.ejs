<div class="modal fade" id="editWalletInfo" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    style="display: none;" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <strong class="modal-title method-name" id="exampleModalLabel">Edit Wallet Info</strong>
                <a href="javascript:void(0)" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">Ã—</span>
                </a>
            </div>
            <div class="card">
                <div class="card-body">
                    <form id="edit-wallet">

                        <div class="row">
                            


                            <div class="col-md-12">
                                <div class="form-group mt-4">
                                    <label for="number" class="font-weight-bold"> Account Number</label>

                                    <input disabled readonly required="" id="number" type="text" class="form-control form-control-lg"
                                        placeholder="Account Number">
                                </div>

                                <div class="form-group mt-4">
                                    <label for="bank" class="font-weight-bold"> Bank Name</label>
                                    <select disabled readonly class="form-control form-control-lg" id="bank">
                                        <%banks.map(bk=>{%>
                                            <option value="<%=bk.code%>">
                                                <%=bk.name%>
                                            </option>
                                            <%})%>
                                    </select>
                                </div>

                            </div>


                            <div class="col-md-12">
                                <div class="form-group">
                                    <button id="proceed" type="submit"
                                        class="btn btn-primary btn-block mt-2 text-center">Verify & Save Edit</button>
                                </div>
                            </div>

                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>
</div>