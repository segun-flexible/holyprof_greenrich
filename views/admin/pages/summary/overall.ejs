<%- include('../../partials/header'); %>




    <div class="container-fluid dashboard-content">

        <!-- start page title -->
        <div class="row">
            <div class="col-12">
                <div class="page-title-box">
                    <h4 class="page-title">
                        <%= title %>
                    </h4>
                </div>
            </div>
        </div>
        <!-- end page title -->


        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="card">
                    
                    <div class="card-body p-0">
                        <%if(admins.length> 0){ %>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead class="bg-light">
                                        <tr class="border-0">
                                            <th class="border-0">#</th>
                                            <th class="border-0">Name</th>
                                            <th class="border-0">Amount Receieved</th>
                                            <th class="border-0">Amount Paid Out</th>
                                            <th class="border-0">Gross Remaining</th>
                                            
        
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <%admins.map((pk,index)=>{ %>
                                            <tr>
                                                <td>
                                                    <%=index + 1 %>
                                                </td>
        
                                                <td>
                                                    <%=pk.username %>
                                                </td>
                                                <td>
                                                    <%=websiteDetails.website_currency%><%=pk.totalInvestment.toLocaleString() %> 
                                                </td>
                                                <td>
                                                    <%=websiteDetails.website_currency%><%=pk.totalWithdrawal.toLocaleString()%>
                                                </td>
        
                                                
                                                <td>
                                                    <%=websiteDetails.website_currency%><%=(pk.totalInvestment - pk.totalWithdrawal).toLocaleString()%>
                                                </td>
        
                                                
        
        
                                            </tr>

                                            <% }) %>
        
                                                <tr>
                                                    <td colspan="9"></td>
                                                </tr>
                                    </tbody>
        
                                </table>
        
                                
        
                            </div>
                            <% }else{%>
                                <h3 class="text-center pt-3">No Summary At The Moment</h3>
                                <%}%>
                    </div>
                </div>
            </div>
        </div>



        
    </div>

<%-include('../../partials/footer'); %>